<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.katarina.spring.mapper.MapperTemplate">

    <select id="queryTemplate" resultType="com.katarina.spring.entity.EntityTemplate">
        SELECT #{userName} AS `user_name`, #{passWord} AS `pass_word` FROM DUAL LIMIT 1
    </select>

</mapper>
